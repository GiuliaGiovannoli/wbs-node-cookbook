CREATE TABLE Recipes (
    id              SERIAL PRIMARY KEY,
    slug            VARCHAR(20),
    title           VARCHAR(50),
    author          VARCHAR(50),
    description     VARCHAR(2000),
  	ingredients		VARCHAR(1000),
    category        VARCHAR(50)
);

